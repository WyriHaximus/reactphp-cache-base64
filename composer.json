{
    "name": "wyrihaximus/react-cache-base64",
    "description": "base64 decorator for react/cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "react/cache": "^1.0.0",
        "thecodingmachine/safe": "^2 || ^1.3"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^5.0.11"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\React\\Cache\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true,
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.1.1"
        },
        "sort-packages": true
    },
    "extra": {
        "unused": [
            "react/cache",
            "thecodingmachine/safe"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
